<div class="add-accounts">
  <a class="btn btn-success" type="button">Add Meetup Account</a>
</div>
<table class="table">
  <thead>
    <tr>
      <th>#</th>
      <th>Provider</th>
      <th>UID</th>
    </tr>
  </thead>
  <tbody>
    <% current_user.accounts.each_with_index do |account, index| %>
    <tr>
      <td><%= index %></td>
      <td><%= account.provider %></td>
      <td><%= account.provider_uid %></td>
    </tr>
    <% end %>
  </tbody>
</table>